server {
    listen       80;
    server_name  localhost;
    
    location / {
        root   /usr/share/nginx/html;
        index  insert.html index.html index.html   
    }

    location /person {
        proxy_pass   http://backend:5000;
    }

    location /persons {
        proxy_pass   http://backend:5000;
    }


}
